{
  "name": "html-nest-rule",
  "version": "0.0.1",
  "description": "nesting rule realization for HTML spec",
  "main": "index.js",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "emmet": "^1.3.2",
    "fecs": "^0.4.4",
    "htmlcs": "^0.1.12",
    "istanbul": "^0.3.2",
    "jasmine-node": "^1.14.5"
  },
  "scripts": {
    "lint": "fecs -v && fecs lib test index.js --type=js",
